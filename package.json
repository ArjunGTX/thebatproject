{
  "name": "thebatproject",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run pretty:check && npm run lint && npm run minify:images && next dev --turbopack",
    "build": "npm run pretty:check && npm run lint && npm run minify:images && next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "pretty": "prettier --write \"./**/*.{json,tsx,ts,css,js,html,scss,yml}\"",
    "pretty:check": "prettier \"./**/*.{json,tsx,ts,css,js,html,scss,yml}\" --check",
    "minify:images": "npx tsx ./scripts/minify-images.ts",
    "build:ci": "npx @cloudflare/next-on-pages@1"
  },
  "dependencies": {
    "next": "15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.7",
    "@eslint/eslintrc": "^3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-format": "^0.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-security": "^3.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
